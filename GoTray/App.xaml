<Application
    x:Class="GoTray.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:goTrayFeed="using:GoTrayFeed"
    xmlns:goTrayUtils="using:GoTrayUtils">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Common/StandardStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <x:String x:Key="AppName">Go</x:String>
            <goTrayFeed:GoTrayFeedSource x:Key="GoTrayFeedSource"/>
            <goTrayUtils:BooleanToVisibilityConverter x:Key="BoolToVisiConv"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
